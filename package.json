{
  "name": "inquirer-plugins",
  "private": true,
  "repository": "https://github.com/Jensen95/inquirer-plugins.git",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --cache-dir=.turbo",
    "changeset": "changeset",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" \"**/package.json\"",
    "lint": "turbo run lint",
    "release": "changeset publish"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.11"
  },
  "devDependencies": {
    "@turbo/gen": "^2.1.3",
    "eslint": "^9.33.0",
    "eslint-config-custom": "*",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.8",
    "turbo": "^2.1.3"
  },
  "packageManager": "npm@9.5.1"
}
